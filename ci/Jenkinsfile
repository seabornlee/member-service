pipeline {
    agent any

    stages {
        stage('docker-compose-stop') {
            steps {
                sh './stopbydocker.sh'
            }
        }
        stage('docker-compose-install') {
            steps {
                sh './buildbydocker.sh'
            }
        }
        stage('docker-compose-up') {
            steps {
                sh './runbydocker.sh'
            }
        }
    }
}
